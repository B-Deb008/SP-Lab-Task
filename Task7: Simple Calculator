#include<stdio.h>
int main()
{
    char ch;
    int a,b;
    printf("Enter an operator: ");
    scanf("%c",&ch);
       printf("Enter two numbers: ");
    scanf("%d%d",&a,&b);

    switch(ch)
    {
    case '+':
        printf("%d", a+b);
        break;
    case '-':
        printf("%d", a-b);
        break;
    case '*':
        printf("%d",a*b);
        break;
    case '/':
        printf("%d", a/b);
        break;
    default:
        printf("Invalid operator!");
    }
    return 0;
}
